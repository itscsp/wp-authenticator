{
    "name": "wp-authenticator/jwt-plugin",
    "description": "WordPress JWT Authentication Plugin using Firebase JWT",
    "type": "wordpress-plugin",
    "require": {
        "php": ">=7.4",
        "firebase/php-jwt": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "WPAuth\\": "includes/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "stable"
}
